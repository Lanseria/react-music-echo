.m-list
  position: fixed
  right: 0
  bottom: 0
  z-index: 200
  width: 460px
  transform: translate3d(0, 100%, 0)
  transition: all .3s ease-in-out
  &.active
    transform: translate3d(0, 0, 0)
  .list-header
    height: 40px
    line-height: 40px
    padding: 0 18px
    background: #f3f5f7
    border-bottom: 1px solid rgba(7, 17, 27, 0.1)
    .title
      float: left
      font-size: 14px
      color: rgb(0, 17, 27)
    .empty
      float: right
      font-size: 12px
      color: #6ed56c
  .list-content
    padding: 0 18px
    max-height: 540px
    overflow: scroll
    background: #fff
    li
      margin: 10px auto 10px
      height: 21px
      border-bottom: 1px solid #f4f4f4
      padding-bottom: 4px
      font: 400 14px/20px taotao,'Microsoft Yahei'
      a
        color: #999
        cursor: pointer
      .song
        color: #999
        &.active
          color: #666